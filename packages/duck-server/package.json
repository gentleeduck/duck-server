{
  "$schema": "https://json.schemastore.org/package.json",
  "engines": {
    "node": ">=22.0.0"
  },
  "version": "0.0.10",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": "./src/**/index.ts"
  },
  "module": "./dist/index.js",
  "name": "@gentleduck/rpc",
  "packageManager": "pnpm@10.10.0",
  "private": false,
  "description": "GentleDuck RPC server utilities.",
  "keywords": [
    "gentleduck",
    "rpc",
    "hono",
    "validation",
    "server"
  ],
  "license": "MIT",
  "author": "GentleDuck (https://www.gentleduck.org)",
  "homepage": "https://github.com/gentleeduck/duck-ui/tree/master/packages/duck-docs#readme",
  "bugs": {
    "url": "https://github.com/gentleeduck/duck-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gentleeduck/duck-ui.git",
    "directory": "packages/duck-docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "bench": "tsx benchmarks/run.ts",
    "dev": "nodemon --watch . --exec tsx ./examples/basic-usage/server.ts",
    "check-types": "tsc -p tsconfig.json --noEmit --pretty false --skipLibCheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "cbor-x": "^1.6.0"
  },
  "peerDependencies": {
    "hono": "^4.0.0",
    "zod": "^3.0.0 || ^4.0.0",
    "valibot": "^1.0.0",
    "arktype": "^2.0.0",
    "@sinclair/typebox": "^0.30.0"
  },
  "peerDependenciesMeta": {
    "hono": {
      "optional": false
    },
    "zod": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "arktype": {
      "optional": true
    },
    "@sinclair/typebox": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.8",
    "@sinclair/typebox": "^0.34.47",
    "@trpc/server": "^11.8.1",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "arktype": "^2.1.29",
    "autocannon": "^8.0.0",
    "hono": "^4.11.3",
    "nodemon": "^3.1.11",
    "tsdown": "0.19.0-beta.4",
    "tsx": "^4.21.0",
    "valibot": "^1.2.0",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  }
}
